                  </xsl:when>
                  <xsl:when test="*/*[local-name()='code']">
                    <xsl:value-of select="*/*[local-name()='code']"/>
                  </xsl:when>
                  <xsl:when test="starts-with(*/@rdf:about,'http://id.loc.gov/vocabulary/marcauthen/')">
