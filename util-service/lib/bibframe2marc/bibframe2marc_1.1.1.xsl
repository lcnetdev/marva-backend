            <xsl:attribute name="ind1">
              <xsl:text> </xsl:text>
            </xsl:attribute>
            <xsl:attribute name="ind2">
              <xsl:text> </xsl:text>
            </xsl:attribute>
            <xsl:for-each select="$vAdminMetadata/bf:descriptionAuthentication">
