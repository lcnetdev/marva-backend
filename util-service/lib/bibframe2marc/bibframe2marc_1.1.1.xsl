      <xsl:apply-templates select="bf:Work/bf:language/bf:Language[bf:source]" mode="generate-041">
        <xsl:with-param name="vRecordId" select="$vRecordId"/>
        <xsl:with-param name="vAdminMetadata" select="$vAdminMetadata"/>
